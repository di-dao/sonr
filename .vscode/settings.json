{
  "gitlens.views.formats.commits.label": "${❰ tips ❱➤  }${message}",
  "gitlens.views.formats.commits.description": "${author, }${agoOrDate}${tips}",
  "gitlens.views.commitDetails.files.layout": "tree",
  "gitlens.views.commitDetails.files.compact": false,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#65cbe9",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#37474F",
      "bold": true,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "!**/.git/hooks": true,
    ".dart_tool": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true,
    "**/.packages": true,
    "**/.project": true,
    "**/.settings": true,
    "**/largeFolder": true,
    "**/pubspec.lock": true,
    "**pubspec.lock": true,
    "app.iml": true,
    "Contents.json": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*plugins/": true,
    "**/**/*.pb.go": true,
    "**/**/*.pb.gw.go": true,
    "**/**/types/*_test.go": true,
    "LICENSE": true,
    "proto/buf.lock": true,
    "**/.circleci": true,
    "**/.compodocrc*": true,
    "**/.editorconfig": true,
    "**/.gitattributes": true,
    "**/.github": true,
    "**/.gitkeep": true,
    "**/.gitignore": true,
    "**/.npmrc": true,
    "**/.prettierignore": true,
    "**/.prettierrc*": true,
    "**/.scannerwork": true,
    "**/.eslint*": true,
    "**/.docker*": true,
    "**/docker": true,
    "**/.nvmrc": true,
    "**/.vscodeignore": true,
    "**/.vscode": true,
    "**/CHANGELOG.md": true,
    "**/README.md": true,
    "**/LICENSE.md": true,
    "**/angular.json": true,
    "**/workspace.json": true,
    "**/jest-mocks.ts": true,
    "**/jest.config.js": true,
    "**/jest.preset.js": true,
    "**/node_modules": true,
    "**/nx.json": true,
    "**/package-lock.json": true,
    "**/package.json": true,
    "**/sonar-project.properties": true,
    "**/tsconfig*": true,
    "**/tslint*": true,
    "**/babel*": true,
    "**/.babel*": true,
    "**/webpack*": true,
    "**/typings.d.ts": true,
    "**/*.yaml": true,
    "**/Makefile": true,
    "**/*.sh": true
  },
  "svg.preview.background": "custom",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnType": true
  },
  "go.autocompleteUnimportedPackages": true,
  "go.formatTool": "default",
  "go.lintTool": "golint",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ],
    "comments": ""
  },
  "explorer.excludeGitIgnore": true,
  "editor.fontFamily": "FiraCode",
  "hide-files.files": [
    "snap",
    "types",
    ".devcontainer",
    "tools",
    "testutil",
    "go.sum",
    ".swm",
    "build"
  ],
  "DockerRun.DisableDockerrc": true,
  "DockerRun.Containers": [],
  "doppler.autocomplete.enable": true,
  "doppler.hover.enable": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#7cd8f9",
    "activityBar.background": "#7cd8f9",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#f52abf",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#7cd8f9",
    "statusBar.background": "#4bc9f7",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#1abaf5",
    "statusBarItem.remoteBackground": "#4bc9f7",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#4bc9f7",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#4bc9f799",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#4BC9F7"
}
