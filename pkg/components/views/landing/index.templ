package landing

import (
	"github.com/labstack/echo/v4"
	blocks "github.com/sonrhq/sonr/pkg/components"
)

templ Homepage(ctx echo.Context) {
	@blocks.RootLayout() {
		<div class="mx-auto max-w-7xl bg-gradient-to-br from-stone-950 via-black to-stone-900">
			@landingNavbar()
			@heroSection()
			@logosSection()
			@featuresSection()
			@integrationsSection()
			@testimonialsSection()
			@ctaSection()
			@landingFooter()
		</div>
	}
}

