{
  "explorer.excludeGitIgnore": true,
  "editor.formatOnSave": true,
  "[templ]": {
    "editor.defaultFormatter": "a-h.templ"
  },
  "files.exclude": {
    "!**/.git/hooks": true,
    "**/.classpath": true,
    "**/.trunk": false,
    "**/.devcontainer": true,
    "**/api": false,
    "**/docs": true,
    "**/.DS_Store": true,
    "**/.factorypath": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.packages": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.svn": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true,
    "**/*_templ.go": true,
    "**/**.md": true,
    "**/**.pb.go": false,
    "**/CODE_OF_CONDUCT.md": true,
    "**/codecov.yml": true,
    "**/CVS": true,
    "**/go.sum": true,
    "**/*.pb.go": false,
    "**/*.pb.gw.go": true,
    "**/SECURITY.md": true,
    "**/go.work": true,
    "**/go.work.sum": true,
    "**/largeFolder": true,
    "**/LICENSE": true,
    "**/pubspec.lock": true,
    "**/Thumbs.db": true,
    "**pubspec.lock": true,
    "app.iml": true,
    "Contents.json": true
  },
  "yaml.schemas": {
    "https://taskfile.dev/schema.json": "file:///Users/pradn/Developer/sonr/tasks/*.yml",
    "https://json.schemastore.org/pull-request-labeler.json": "file:///Users/pradn/Developer/sonr/.github/labeler.yml"
  },
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "explorer.sortOrderLexicographicOptions": "unicode",
  "files.autoGuessEncoding": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveWhenNoErrors": true,
  "search.quickOpen.includeSymbols": true,
  "breadcrumbs.symbolSortOrder": "type",
  "terraform.languageServer.ignoreSingleFileWarning": true,
  "helium-icon-theme.showWelcomeMessage": false
}
