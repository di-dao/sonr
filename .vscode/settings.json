{
  "gitlens.views.formats.commits.label": "${❰ tips ❱➤  }${message}",
  "gitlens.views.formats.commits.description": "${author, }${agoOrDate}${tips}",
  "gitlens.views.commitDetails.files.layout": "tree",
  "gitlens.views.commitDetails.files.compact": false,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#65cbe9",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#37474F",
      "bold": true,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".vscode/": true,
    ".trunk/": true,
    ".devcontainer/": true,
    ".markdownlint.json": true,
    ".golangci.yml": true,
    "CHANGELOG.md": true,
    "CODE_OF_CONDUCT.md": true,
    "codecov.yml": true,
    "LICENSE": true,
    "sonar-project.properties": true,
    "go.sum": true,
    "buf.work.yaml": true
  },
  "svg.preview.background": "custom",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnType": true
  },
  "go.formatTool": "default",
  "go.lintTool": "golint",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "explorer.excludeGitIgnore": true,
  "DockerRun.DisableDockerrc": true,
  "DockerRun.Containers": [],
  "doppler.autocomplete.enable": true,
  "doppler.hover.enable": true,
  "peacock.color": "#7900f7",
  "a-file-icon-vscode.arrowTheme": "triangle",
  "a-file-icon-vscode.folderTheme": "specific",
  "a-file-icon-vscode.folderColor": "#ffd0aa",
  "a-file-icon-vscode.activeIconPacks": [
    "angular",
    "ngrx",
    "nest",
    "react",
    "redux",
    "recoil",
    "vue",
    "vuex",
    "rails",
    "phalcon",
    "volt"
  ],
  "a-file-icon-vscode.opacity": 1,
  "a-file-icon-vscode.saturation": 1,
  "editor.fontFamily": "Iosevka Custom",
  "terminal.integrated.fontFamily": "Iosevka Custom",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#932bff",
    "activityBar.background": "#932bff",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#ff7d00",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#932bff",
    "statusBar.background": "#7900f7",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#932bff",
    "statusBarItem.remoteBackground": "#7900f7",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#7900f7",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#7900f799",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.remoteColor": "#d31135",
  "hidefiles.configurationType": "global",
  "hidefiles.selectedProfile": "Strict"
}
