{
  "gitlens.views.formats.commits.label": "${❰ tips ❱➤  }${message}",
  "gitlens.views.formats.commits.description": "${author, }${agoOrDate}${tips}",
  "gitlens.views.commitDetails.files.layout": "tree",
  "gitlens.views.commitDetails.files.compact": false,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#65cbe9",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#37474F",
      "bold": true,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "!**/.git/hooks": true,
    ".dart_tool": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true,
    "**/.packages": true,
    "**/.project": true,
    "**/.settings": true,
    "**/largeFolder": true,
    "**/pubspec.lock": true,
    "**pubspec.lock": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*plugins/": true,
    "**/**/types/*_test.go": true,
    "proto/buf.lock": true,
    "**/.circleci": true,
    "**/.compodocrc*": true,
    "**/.editorconfig": true,
    "**/.gitattributes": true,
    "**/.gitkeep": true,
    "**/.gitignore": true,
  },
  "svg.preview.background": "custom",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnType": true
  },
  "go.formatTool": "default",
  "go.lintTool": "golint",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "explorer.excludeGitIgnore": true,
  "DockerRun.DisableDockerrc": true,
  "DockerRun.Containers": [],
  "doppler.autocomplete.enable": true,
  "doppler.hover.enable": true,
  "peacock.color": "#077aa2",
  "a-file-icon-vscode.arrowTheme": "triangle",
  "a-file-icon-vscode.folderTheme": "specific",
  "a-file-icon-vscode.folderColor": "#ffd0aa",
  "a-file-icon-vscode.activeIconPacks": [
    "angular",
    "ngrx",
    "nest",
    "react",
    "redux",
    "recoil",
    "vue",
    "vuex",
    "rails",
    "phalcon",
    "volt"
  ],
  "a-file-icon-vscode.opacity": 1,
  "a-file-icon-vscode.saturation": 1,
  "editor.fontFamily": "Iosevka Custom",
  "terminal.integrated.fontFamily": "Iosevka Custom",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ee294e",
    "activityBar.background": "#ee294e",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#165907",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#ee294e",
    "statusBar.background": "#d31135",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#ee294e",
    "statusBarItem.remoteBackground": "#d31135",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#d31135",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#d3113599",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.remoteColor": "#d31135"
}
