{
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false,
        "editor.defaultFormatter": "Dart-Code.dart-code"
    },
    "dart.showDartDeveloperLogs": true,
    "dart.lineLength": 150,
    "dart.promptToGetPackages": false,
    "dart.runPubGetOnPubspecChanges": true,
    "[go]": {
        "editor.formatOnSave": true,
        "editor.snippetSuggestions": "none",
    },
    "[go.mod]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        },
    },
    "go.lintFlags": [
        "--fast",
        "--allow-parallel-runners"
    ],
    "gopls": {
        "usePlaceholders": true,
        "completeUnimported": true,
    },
    "go.lintTool": "golangci-lint",
    "go.autocompleteUnimportedPackages": true,
    "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
    "go.toolsManagement.autoUpdate": true,
    "files.exclude": {
        "**/package-lock.json": true,
        "**/plasmic.lock": true,
        ".dart_tool": true,
        "**/**.pb.go": true,
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.flutter-plugins": true,
        "**/.flutter-plugins-dependencies": true,
        "**/.packages": true,
        "**/.project": true,
        "**/.settings": true,
        "**/go.sum": true,
        "**/pubspec.lock": true,
        "**pubspec.lock": true,
        "**/node_modules": true,
        "app.iml": true,
        "Contents.json": true,
        ".devcontainer": true,
        // Project Related
        "**/app": true,
        "**/design": true,
        "**/sonr.wiki": true,
    },
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "keep",
        "gitlens.codeLens.scopes": [
            "document"
        ],
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        }
    },
    "protoc": {},
    "better-comments.tags": [
        {
            "tag": "^",
            "color": "#c982ff",
            "strikethrough": false,
            "underline": true,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "/ ####",
            "color": "#98C379",
            "strikethrough": false,
            "underline": true,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": true
        },
        {
            "tag": "/ ###",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": true
        },
        {
            "tag": "/ ##",
            "color": "#c982ff",
            "strikethrough": false,
            "underline": true,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": " ####",
            "color": "#98C379",
            "strikethrough": false,
            "underline": true,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": true
        },
        {
            "tag": " ###",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": true
        },
        {
            "tag": " ##",
            "color": "#c982ff",
            "strikethrough": false,
            "underline": true,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "#FFEEEE",
            "bold": false,
            "italic": false
        }
    ],
    "dart.autoImportCompletions": false,
    "dart.previewFlutterUiGuidesCustomTracking": true,
    "dart.allowAnalytics": false,
    "dart.analyzeAngularTemplates": false,
    "dart.devToolsReuseWindows": false,
    "dart.flutterHotRestartOnSave": false,
    "dart.flutterSelectDeviceWhenConnected": false,
    "dart.showDartPadSampleCodeLens": false,
    "dart.showDevToolsDebugToolBarButtons": false,
    "dart.showTodos": false,
    "dart.useKnownChromeOSPorts": false,
    "dart.openDevTools": "flutter",
    "dart.warnWhenEditingFilesInPubCache": false,
    "dart.previewLsp": true,
    "dart.warnWhenEditingFilesOutsideWorkspace": false,
    "go.languageServerFlags": [
        "-rpc.trace",
        "serve",
        "--debug=localhost:6060"
    ],
    "go.useLanguageServer": true,
    "material-icon-theme.opacity": 0.75,
    "material-icon-theme.saturation": 0.75,
    "material-icon-theme.files.associations": {
        "*.proto": "xml"
    },
    "material-icon-theme.folders.associations": {
        "Grida": "interface",
        "bind": "dist",
        "cli": "dist",
        "rpc": "dist",
        "internal": "core",
        "CD": "pipe",
        "pkg": "src",
        "cmd": "core",
        "proto": "json",
        "common": "",
        "config": "",
        "util": "",
        "service": "",
        "workflows": "serverless",
        "electron": "examples",
        "flutter": "examples"
    },
    "github.codespaces.defaultExtensions": [
        "GitHub.codespaces",
        "GitHub.vscode-pull-request-github"
    ],
    "liveServer.settings.multiRootWorkspaceName": "üèõ  Sonr"
}
