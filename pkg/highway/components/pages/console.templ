package pages

import (
	"github.com/labstack/echo/v4"

	"github.com/sonrhq/sonr/pkg/highway/components/layouts"
	"github.com/sonrhq/sonr/pkg/highway/shared"
)

templ consoleView() {
	@layouts.ConsolePage()
}

func Console(c echo.Context) error {
	cmp := consoleView()
	return shared.RenderPage(c, cmp)
}
