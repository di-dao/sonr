{
  "manifest_version": 3,
  "name": "React - Data Fetching Starter Template",
  "description": "Starter template for a React-based data fetching extension.",
  "version": "1.0.0",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_icon": {
      "32": "icon.png"
    },
    "default_title": "Open Page"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://reddit.com/*"]
}
