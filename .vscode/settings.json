{
	"explorer.excludeGitIgnore": false,
	"editor.formatOnSave": true,
	"[templ]": {
		"editor.defaultFormatter": "a-h.templ"
	},
	"tailwindCSS.includeLanguages": {
		"templ": "html"
	},
	"files.exclude": {
		"**/.git": true,
		"Icon": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"!**/.git/hooks": true,
		".dart_tool": true,
		"**/.classpath": true,
		"**/.factorypath": true,
		"**/.flutter-plugins": true,
		"**/.flutter-plugins-dependencies": true,
		"**/.packages": true,
		"**/.project": true,
		"**/.settings": true,
		"**/largeFolder": true,
		"**/**_templ.go": false,
		"**/pubspec.lock": true,
		"**pubspec.lock": true,
		"app.iml": true,
		"Contents.json": true,
		"**/.trunk/*out/": true,
		"**/.trunk/*actions/": true,
		"**/.trunk/*logs/": true,
		"**/.trunk/*plugins/": true,
		"**/node_modules": true,
		"**/CODE_OF_CONDUCT.md": true,
		"**/codecov.yml": true,
		"**/SECURITY.md": true,
		"**/LICENSE": true,
		".devcontainer": true,
		"identity": true,
		"service": true,
		"web": true,
		"**/.next": true,
		"**/.vercel": true,
		"**/bun.lockb": true,
		"**/next-env.d.ts": true,
		"**/postcss.config.js": true,
		"**/package-lock.json": true,
		"**/.eslintrc.json": true,
		"**/go.work": true,
		"**/go.work.sum": true,
		"**/go.sum": true,
		"**/**.pb.go": true,
	}
}
