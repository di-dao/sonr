{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "outDir": "./lib",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": false,
    "strictPropertyInitialization": false,
    "strict": false,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
